{
  "project": {
    "organization": "com.github.madoc",
    "extraScalacOptions": ["-unchecked", "-deprecation", "-encoding", "utf8"],
    "scalaVersion": "2.12.3",
    "version": "0.1-SNAPSHOT",
    "gitIgnore": [".idea_modules", "project/project", ".idea", "*.iml", "*#", "target", "project.vim", "*~", "project/target", ".c9", "src/autogenerated", ".DS_Store", "out", "logs", "conf/local.conf", "test-results.xml", "tmp", ".classpath", ".project", "RUNNING_PID", ".settings", "atlassian-ide-plugin.xml"],
    "sbtVersion": "1.0.1",
    "extraJavaOptions": [{
      "options": ["-Xdebug", "-Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=5005"]
    }, {
      "contexts": ["Test", "run"],
      "options": ["-Djava.awt.headless=true"]
    }],
    "libraries": ["scalatest"],
    "plugins": ["scoverage"]
  },
  "refs": {
    "libraryRefs": {
      "akka": [{
        "group": "com.typesafe.akka",
        "artifact": "akka-actor",
        "version": "2.4.16",
        "withSources": true
      }],
      "scala-xml": [{
        "group": "org.scala-lang.modules",
        "artifact": "scala-xml",
        "version": "1.0.6",
        "withSources": true
      }],
      "scalatest": [{
        "version": "3.0.1",
        "withSources": true,
        "configuration": "test",
        "group": "org.scalatest",
        "artifact": "scalatest"
      }]
    },
    "pluginRefs": {
      "scoverage": [{
        "group": "org.scoverage",
        "artifact": "sbt-scoverage",
        "version": "1.5.1"
      }]
    },
    "resolverRefs": {
      "sonatype-rel": [{
        "name": "Sonatype Releases",
        "url": "http://oss.sonatype.org/content/repositories/releases"
      }],
      "sonatype-snap": [{
        "name": "Sonatype Snapshots",
        "url": "http://oss.sonatype.org/content/repositories/snapshots"
      }]
    }
  }
}
